{
    "name": "adryanev/depot-yii",
    "description": "Depot Air Minum",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap4": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "nesbot/carbon": "^2.17",
        "mdmsoft/yii2-admin": "~2.0",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "dominus77/yii2-sweetalert2-widget": "~1.3",
        "yii2mod/collection": "*",
        "iiifx-production/yii2-autocomplete-helper": "^2.0",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "pusher/pusher-php-server": "^4.1",
        "omnilight/yii2-shopping-cart": "*",
        "kartik-v/yii2-widget-select2": "dev-master",
        "npm-asset/bootstrap-notify": "^3.0",
        "npm-asset/animate.css": "^4.0",
        "phpoffice/phpspreadsheet": "^1.16",
        "kartik-v/yii2-datecontrol": "dev-master",
        "kartik-v/yii2-widget-datepicker": "@dev"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4",
        "dealerdirect/phpcodesniffer-composer-installer": "*",
        "yiisoft/yii2-coding-standards": "^2.0",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "3.*"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
